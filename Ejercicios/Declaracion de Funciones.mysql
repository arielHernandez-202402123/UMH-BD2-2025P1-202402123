DELIMITER //
CREATE FUNCTION fn_zero_division (p_valor1 DOUBLE, p_valor2 DOUBLE)
RETURNS DOUBLE DETERMINISTIC
BEGIN
	DECLARE division DOUBLE DEFAULT 0;
    
    IF p_valor2 != 0 then
		SET division = p_valor1 / p_valor2;
    END IF;

    RETURN division;
    
END;

SELECT fn_get_producto(10, 0 ) division;
