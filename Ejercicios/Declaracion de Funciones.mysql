DELIMITER //
CREATE FUNCTION fn_get_producto (p_valor1 DOUBLE, p_valor2 DOUBLE)
RETURNS DOUBLE DETERMINISTIC
BEGIN
	DECLARE producto DOUBLE;
    
    SET producto = p_valor1 * p_valor2;
    RETURN producto;
    
END;


SELECT fn_get_producto(3, 3 ) producto;

